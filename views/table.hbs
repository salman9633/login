<div class="container mt-5">
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">MOVIE</th>
                <th scope="col">IMBD</th>
                <th scope="col">RELEASED</th>
                <th scope="col">DIRECTOR</th>
            </tr>
        </thead>
        <tbody>
            {{#each table}}
            <tr>
                <td>{{this.MOVIE}}</td>
                <td>{{this.IMBD}}</td>
                <td>{{this.RELEASED}}</td>
                <td>{{this.DIRECTOR}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>